    "processor": "cpu",
    "framework": {
      "framework_name": "pytorch",
      "framework_version": "1.12.0"
    },
    "shadow_config": {
      "enabled": false
    }
  },
  "serving_regions": [
    "us-east-1",
    "us-west-2"
  ]
}
